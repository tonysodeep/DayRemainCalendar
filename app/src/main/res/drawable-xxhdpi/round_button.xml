<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="oval">
            <solid android:color="@color/main_tab_bg"/>
            <stroke android:width="3dp" android:color="@color/main_un_selected_color_border_cir"/>
        </shape>
    </item>
    <item android:state_selected="true">
        <shape android:shape="oval">
            <solid android:color="@color/main_tab_bg"/>
            <stroke android:width="3dp" android:color="@color/main_selected_color_border_cir"/>
        </shape>
    </item>
</selector>
