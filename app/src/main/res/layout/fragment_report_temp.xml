<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ReportFragmentTemp"
    android:background="@color/frag_report_bg">

    <!-- TODO: Update blank fragment layout -->
    <TextView
        android:id="@+id/heding"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="48dp"
        android:layout_marginStart="48dp"
        android:layout_width="0dp"
        android:layout_marginEnd="50dp"
        android:layout_height="wrap_content"
        android:background="@drawable/fm_home_border"
        android:textColor="@color/frag_report_text"
        android:textSize="22sp"
        android:text="All Events" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/report_rv"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/heding"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginTop="48dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>
